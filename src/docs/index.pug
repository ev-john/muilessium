doctype html
html(lang='en')
    head
        meta(http-equiv='content-type' content='text/html;charset=UTF-8')
        meta(name='viewport' content='user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1')
        title Muilessium

        link(rel='apple-touch-icon' sizes='180x180' href='./favicons/apple-touch-icon.png')
        link(rel='icon' type='image/png' sizes='32x32' href='./favicons/favicon-32x32.png')
        link(rel='icon' type='image/png' sizes='16x16' href='./favicons/favicon-16x16.png')
        link(rel='manifest' href='./favicons/manifest.json')
        link(rel='mask-icon' href='./favicons/safari-pinned-tab.svg' color='#050505')
        meta(name='theme-color' content='#ffffff')

        link(rel='prefetch' href='./muilessium.min.js?v=' + pkg.version)
        link(rel='prefetch' href='./muilessium-ui.min.css?v=' + pkg.version)
        link(rel='prefetch' href='./muilessium-ui.min.js?v=' + pkg.version)


    body
        .mui-scroll-fix

        .mui-page-container
            .mui-hero-header.-full-height.-grey-4-bg
                .effects
                .content
                    img.mui-image.-logo.-js-lazy-load(data-src='./muilessium.png' alt='Logo' src='data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7')
                    noscript
                        img.mui-image.-logo(src='./muilessium.png' alt='Logo')
                    h2._separate-1
                        span Muilessium&nbsp;
                        span._smaller-5 #{pkg.version}
                    ._separate-1
                        a.mui-button.-white.-black-bg.-f-black.-f-green-3-bg(href='./muilessium/') Docs
                        span &nbsp;
                        a.mui-button.-white.-black-bg.-f-black.-f-green-3-bg(href='./muilessium-ui/') Muilessium-UI Docs
                    h3._separate-top-3._bigger-0 Projects:
                    ._separate-1._text-center
                        a(href='https://sfi0zy.github.io' title='Ivan Bogachev - personal website' target='_blank')
                            img.mui-image.-rounded.-project-logo.-js-lazy-load(data-src='https://raw.githubusercontent.com/sfi0zy/sfi0zy.github.io/source/android-chrome-192x192.png' src='data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7' alt='sfi0zy')
                            noscript
                                img.mui-image.-rounded.-project-logo(src='https://raw.githubusercontent.com/sfi0zy/sfi0zy.github.io/source/android-chrome-192x192.png' alt='sfi0zy')
                        a(href='https://github.com/sfi0zy/html-template' title='HTML template' target='_blank')
                            img.mui-image.-rounded.-project-logo.-js-lazy-load(data-src='https://raw.githubusercontent.com/sfi0zy/html-template/master/src/images/favicon.png' src='data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7' alt='HTML template')
                            noscript
                                img.mui-image.-rounded.-project-logo(src='https://raw.githubusercontent.com/sfi0zy/html-template/master/src/images/favicon.png' alt='HTML template')
                        a(href='https://github.com/sfi0zy/zen-launcher' title='Zen launcher for Google Chrome' target='_blank')
                            img.mui-image.-rounded.-project-logo.-js-lazy-load(data-src='https://raw.githubusercontent.com/sfi0zy/zen-launcher/master/src/images/icon128.png' src='data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7' alt='Zen launcher')
                            noscript
                                img.mui-image.-rounded.-project-logo(src='https://raw.githubusercontent.com/sfi0zy/zen-launcher/master/src/images/icon128.png' alt='Zen launcher')
                .mui-ribbon.-classic.-top.-right: a(href='https://github.com/sfi0zy/muilessium' target='_blank') GitHub

        link(rel='stylesheet' href='./muilessium-ui.min.css?v=' + pkg.version)

        style.
            .mui-image.-logo {
                width: 5rem;
            }

            .mui-image.-project-logo {
                height: 3rem;
                width: 3rem;
                display: inline-block;
                margin: 0 .5rem;
            }

            .mui-ribbon {
                text-transform: none;
            }

        script(src='./muilessium.min.js?v=' + pkg.version)
        script(src='./muilessium-ui.min.js?v=' + pkg.version)

        script.
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

            ga('create', 'UA-53893511-1', 'auto');
            ga('send', 'pageview');

