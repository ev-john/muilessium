    <script>
        document.addEventListener('DOMContentLoaded', function() {
            [].forEach.call(document.querySelectorAll('.state .example'), function(example) {
                var stateClass = example.getAttribute('data-class'),
                    targetChild = 0;

                if (example.childNodes[1]) {
                    targetChild = 1;
                }

                example.childNodes[targetChild].className += ' ' + stateClass;
            });

            prettyPrint();

            Muilessium.Utils.scrollFire(document.getElementById('page-layout-overview'), function() {
                var scrollToTopButton = document.getElementById('scroll-to-top');

                Muilessium.Utils.removeClass(scrollToTopButton, '-hidden');

                Muilessium.Utils.makeElementClickable(scrollToTopButton, Muilessium.Utils.scrollToTop);
            });
        });
    </script>
