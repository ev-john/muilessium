kground-color:#fff}body{display:-ms-flexbox;display:flex;-ms-flex-direction:column;flex-direction:column;min-height:100%;min-width:320px}a{color:#383838}a:visited{color:#383838}:focus{outline:none}div:empty:not(.indicator):not(.mui-navigation-toggle):not(._allow-empty):not(.mui-shadow-toggle):not(.scrollbar){display:none}h1,h2,h3,h4{margin:0;padding:0;line-height:1.2}img{display:block;height:auto;max-width:100%}html{font-family:-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen,Ubuntu,Cantarell,Fira Sans,Droid Sans,Helvetica Neue,sans-serif;font-size:calc(14px + 6 * ((100vw - 400px) / 800));line-height:1.5}@media screen and (max-width:400px){html{font-size:14px}}@media screen and (min-width:1200px){html{font-size:20px}}h1{font-size:3.05em!important}h2{font-size:2.44em!important}h3{font-size:1.95em!important}h4{font-size:1.56em!important}.mui-row{display:-ms-flexbox;display:flex;-ms-flex:0 1 auto;flex:0 1 auto;-ms-flex-direction:row;flex-direction:row;-ms-flex-wrap:wrap;flex-wrap:wrap}.mui-column{-ms-flex:0 0 auto;flex:0 0 auto;-ms-flex-positive:1;flex-grow:1;-ms-flex-preferred-size:0;flex-basis:0;max-width:100%}@media screen and (max-width:400px){.mui-column{-ms-flex-preferred-size:100%;flex-basis:100%;max-width:100%}}.mui-column.-s12{-ms-flex-preferred-size:100%;flex-basis:100%;max-width:100%}@media screen and (min-width:600px){.mui-column.-m3{-ms-flex-preferred-size:25%;flex-basis:25%;max-width:25%}.mui-column.-m9{-ms-flex-preferred-size:75%;flex-basis:75%;max-width:75%}}.mui-badge{font-size:.8em!important;display:inline-block;padding:0 .5em;margin-left:.3em;line-height:1.5;text-align:center;color:#fff;background:#383838;border-radius:0;vertical-align:middle;transform:translateY(-.1em)}.mui-button{display:inline-block;position:relative;padding:0 1em;font-size:16px;font-size:1rem;line-height:2;text-decoration:none;text-align:center;border:none;border-radius:.2rem}.mui-button,.mui-button:visited{color:#fff;background:#6d6d6d}.mui-checkbox>input[type=checkbox]{display:none}.mui-content-container{position:relative;width:90%;max-width:1100px;margin:0 auto}.mui-content-container:after{content:"";display:table;clear:both}.mui-page-container{-ms-flex:1 0 auto;flex:1 0 auto;overflow:auto}.mui-header-logo{display:inline-block;line-height:64px;line-height:4rem}.mui-header-logo>.link{color:#000;text-decoration:none}.mui-header-navigation>.linkslist{list-style:none;margin:0}.mui-header-navigation>.linkslist>.item{display:inline-block;margin:0}.mui-header-navigation>.linkslist>.item>.link{display:inline-block;padding:0 calc((2rem * 2 - 1em) / 2);line-height:64px;line-height:4rem;text-decoration:none;color:#000}.mui-header-navigation.-mobile-version>.linkslist{display:block;visibility:hidden;position:fixed;top:0;right:0;z-index:11;overflow-y:scroll;height:100%;width:80%;padding:16px;padding:1rem;font-size:32px;font-size:2rem;text-align:right;transform:translateX(100%);background:#fff;box-shadow:0 0 10rem rgba(0,0,0,.3)}.mui-header-navigation.-mobile-version>.linkslist>.item{display:block;overflow:hidden;margin-left:0}.mui-header-navigation.-mobile-version>.linkslist>.item:first-of-type{margin-top:64px;margin-top:4rem}.mui-header-navigation.-mobile-version>.linkslist>.item:not(:first-of-type){border-top:1px solid #c6c6c6}.mui-header-navigation.-mobile-version>.linkslist>.item>.link{display:block;padding:8px 16px;padding:.5rem 1rem}.mui-navigation-toggle.-hamburger{display:inline-block;position:relative;float:right;height:64px;height:4rem;line-height:64px;line-height:4rem;padding:0 16px;padding:0 1rem;font-size:32px;font-size:2rem;color:#6d6d6d}.mui-hero-header>.effects{position:absolute;top:0;left:0;height:inherit;width:inherit}.mui-image{margin-left:auto;margin-right:auto}.mui-image.-js-lazy-load{opacity:0}.mui-modal-window{position:fixed;top:0;left:0;height:100%;width:100%;z-index:10;opacity:0}.mui-modal-window>.window{position:absolute;top:50%;left:50%;max-width:80vw;max-height:80vh;transform:translateX(-50%) translateY(-50%);z-index:11}.mui-modal-window>.window>.content{overflow:hidden;border-radius:.2rem}.mui-modal-window>.window>.content.-image .mui-image{max-height:80vh;max-width:80vw}.mui-modal-window>.window>.closeicon{position:absolute;top:-16px;top:-1rem;right:-16px;right:-1rem;height:32px;height:2rem;width:32px;width:2rem;font-size:25.6px;font-size:1.6rem;line-height:32px;line-height:2rem;text-align:center;border-radius:50%;background:#fff;border:2px solid #c6c6c6;color:#6d6d6d;z-index:2;box-sizing:content-box}.mui-modal-window>.mui-shadow-toggle{position:fixed;z-index:10;top:0;left:0;height:100vh;width:100vw}.mui-notification{padding:16px;padding:1rem}.mui-notification.-message{position:fixed;bottom:16px;bottom:1rem;width:320px;width:20rem;max-width:320px;max-width:20rem;border-radius:.3rem;z-index:20;opacity:0}@media screen and (max-width:22rem){.mui-notification.-message{width:calc(100% - 2rem);max-width:calc(100% - 2rem)}}.mui-notification.-message.-left{left:16px;left:1rem}.mui-notification.-message.-right{right:16px;right:1rem}.mui-page-header{position:relative;max-height:64px;max-height:4rem}.mui-page-header:after{content:"";display:table;clear:both}.mui-p{display:block;margin:0 auto;max-width:1100px;text-align:justify;text-align-last:left}.mui-progress-bar>.value{position:absolute;top:0;left:0;height:100%;width:100%;line-height:32px;line-height:2rem;text-align:center;color:#383838}.mui-progress-bar.-radial>.value{line-height:128px;line-height:8rem}.mui-scroll-fix{position:fixed;top:0;left:0;height:100vh;width:100%;z-index:100}.mui-shadow-toggle{position:fixed;left:0;top:0;z-index:10;height:100%;width:100%;background:transparent;opacity:0}.mui-svg-icon{display:inline-block;height:1em;max-height:1em;max-width:1em;transform:translateY(-10%);vertical-align:middle}.mui-svg-icon.-white{fill:#fff}.mui-svg-icon.-black{fill:#000}.mui-svg-sprite{position:fixed}.mui-page-header{padding-left:1rem;width:100%;z-index:1}.mui-image.-logo{float:left;display:inline-block;height:3rem;width:3rem;margin:.5rem 1rem .5rem 0}.mui-header-logo h1{line-height:3.7rem}.mui-header-navigation.-mobile-version .linkslist{background:#000;color:#fff}.example .mui-header-navigation.-mobile-version .linkslist{background:#fff;color:#000}@media screen and (max-width:600px){aside{display:none!important}}.aside-navigation{padding:0 1rem}.aside-navigation a{text-decoration:none}.aside-navigation .sub-list{list-style:none;padding-left:1em}.aside-navigation .sub-list a:before{content:'- '}hr{border:0;height:0;border-top:1px solid #c6c6c6}.anchor-link{text-decoration:none}
    </style>

</head>
<body>
    <!-- scroll-fix -->
    <div class='mui-scroll-fix'></div>
    <!-- /scroll-fix -->



    <!-- page-header -->
    <header class='mui-page-header -black-bg'>
        <!-- content-container -->
        <div class='mui-content-container'>


            <!-- header-logo -->
            <div class='mui-header-logo _left'>
                <img class='mui-image -logo -js-lazy-load' data-src='./muilessium.png' src='data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7' alt='logo'>
                <noscript>
                    <img class='mui-image -logo' src='./muilessium.jpg' alt='Muilessium logo'>
                </noscript>
                <a class='link -white -f-blue-3 _right' href='/'>
                    <h1 class='heading _bigger-1'>{{ project.name }} <span class='_smaller-2'>v{{ project.version }}</span></h1>
                </a>
            </div>
            <!-- /header-logo -->


            <!-- header-navigation -->
            <nav class='mui-header-navigation -mobile-version _right'>
                <div class='mui-navigation-toggle -hamburger -f-blue-3-bg' role='button' aria-label='Open navigation'>
                    <svg class='mui-svg-icon -white'><use xlink:href='#svg-icon--hamburger'></use></svg>
                </div>
                <ul class='linkslist'>
                    <li class='item'><a class='link -white -f-blue-3-bg -f-white' href='#overview' aria-label='Framework overview'>Overview</a></li>
                    <li class='item'><a class='link -white -f-blue-3-bg -f-white' href='#components' aria-label='Components'>Components</a></li>
                    <li class='item'><a class='link -white -f-blue-3-bg -f-white' href='#faq' aria-label='f.a.q.'>F.A.Q.</a></li>
                </ul>
                <div class='mui-shadow-toggle' aria-label='Close navigation'></div>
            </nav>
            <!-- header-navigation -->


        </div>
        <!-- /content-container -->
    </header>
    <!-- /page-header -->




    <!-- content-container -->
    <div class='mui-content-container'>


        <!-- aside navigation -->
        <aside class='mui-column -s12 -m3 _right'>
            <div class='aside-navigation _smaller-1 _line-1-25 _separate-3' role='navigation'>
                <a class='-blue-3 -f-blue-1' href='#overview'>Overview</a>
                <ul class='sub-list _separate-1'>
                    <li><a class='-blue-3 -f-blue-1' href='#cdn'>CDN</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#grid-overview'>Grid</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#page-layout-overview'>Page layout</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#meta-viewport-overview'>Viewport</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#typography-overview'>Typography</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#colors-overview'>Colors</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#helpers-overview'>Helpers</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#globals-overview'>Globals</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#events-overview'>Events</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#store-overview'>Store</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#factory-overview'>Factory</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#controls-overview'>Controls</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#utils-overview'>Utils</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#lazy-loading-images-overview'>Lazy loading</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#icons-overview'>Icons</a></li>
                    <li><a class='-blue-3 -f-blue-1' href='#animations-overview'>Animations</a></li>
                </ul>
                <hr class='_separate-1'>
                <a class='-blue-3 -f-blue-1' href='#components'>Components</a>
                <ul class='sub-list _separate-1'>
                {{#files}}
                    {{#blocks}}
                        <li>
                            <a class='-blue-3 -f-blue-1' href='#{{../file}}'>{{ name }}</a>
                        </li>
                    {{/blocks}}
                {{/files}}
                </ul>
                <hr class='_separate-1'>
                <a class='-blue-3 -f-blue-1' href='#faq'>F.A.Q.</a>
            </div>
        </aside>
        <!-- /aside navigation -->




        <!-- main content -->
        <main class='mui-column -s12 -m9'>
            <!-- content-container -->
            <div class='mui-content-container'>


                <!-- overview -->
                <div class='_separate-3'>
                    <div class='mui-row'>
                        <a class='anchor-link -blue-3 -f-blue-1' href='#overview'>
                            <h2 class='_bigger-2' id='overview'>Overview</h2>
                        </a>
                    </div>

                    <p class='mui-p _separate-1'>Muilessium is a UI framework for simple static websites (personal blogs, landings e.t.c.). It consists of over 50 components, a lot of utilities, helpers and syntax wrappers to make your code cleaner. This project can be used as a dependency or you can fork it and use as boilerplate for your project.</p>

                    <div class='_separate-1'>
                        <a class='mui-button -green-3-bg -white -f-green-1-bg' href='https://github.com/sfi0zy/muilessium'>GitHub</a>
                        <a class='mui-button -red-3-bg -white -f-red-1-bg' href='https://www.npmjs.com/package/muilessium'>NPM</a>
                    </div>
                </div>
                <!-- /overview -->


                <!-- CDN -->
                <div class='_separate-5'>
                    <div class='mui-row'>
                        <a class='anchor-link -blue-3 -f-blue-1' href='#cdn'>
                            <h3 class='_bigger-2' id='cdn'>CDN</h3>
                        </a>
                    </div>
                    <div class='_separate-1'>
                        <h4 class='_bigger-0 _separate-1'>Unpkg</h4>
                        <a href='https://unpkg.com/muilessium/dist/js/muilessium.min.js' class='mui-button -black-bg -f-grey-3-bg'>JS</a>
                        <a href='https://unpkg.com/muilessium/dist/css/muilessium.min.css' class='mui-button -black-bg -f-grey-3-bg'>CSS</a>

                        <h4 class='_bigger-0 _separate-1'>jsDelivr</h4>
                        <a href='https://cdn.jsdelivr.net/npm/muilessium/dist/js/muilessium.min.js' class='mui-button -orange-3-bg -f-orange-1-bg'>JS</a>
                        <a href='https://cdn.jsdelivr.net/npm/muilessium/dist/css/muilessium.min.css' class='mui-button -orange-3-bg -f-orange-1-bg'>CSS</a>
                    </div>
                </div>
                <!-- /CDN -->


                <!-- grid overview -->
                <div class='_separate-5'>
                    <div class='mui-row'>
                        <a class='anchor-link -blue-3 -f-blue-1' href='#grid-overview'>
                            <h3 class='_bigger-2' id='grid-overview'>Grid</h3>
                        </a>
                    </div>
                    <div class='_smaller-2 -grey-4'>/src/less/components/grid-flex.less</div>

                    <p class='mui-p _separate-1'>Muilessium uses a grid with 12 columns based on flexbox. Rows and columns have classes .mui-row and .mui-column and columns can have options -s* for a small screen, -m* for a medium screen and -l* for a large screen. Default breakpoints are 400, 600 and 1200px. Its behavior is similar to the grid from flexboxgrid.com, but it uses RSCSS-style class names.</p>

                    <div class='grid-preview _separate-2'>
                        <div class='mui-row'>
                            <div class='mui-column -s1 -green-5-bg -grey-1 _text-center _line-2r _smaller-1'>-s1</div>
                            <div class='mui-column -s11 -green-1-bg -grey-5 _text-center _line-2r _smaller-1'>-s11</div>
                        </div>
                        <div class='mui-row'>
                            <div class='mui-column -s3 -green-5-bg -grey-1 _text-center _line-2r _smaller-1'>-s3</div>
                            <div class='mui-column -s9 -green-1-bg -grey-5 _text-center _line-2r _smaller-1'>-s9</div>
                        </div>
                        <div class='mui-row'>
                            <div class='mui-column -s6 -m4 -l6 -green-5-bg -grey-1 _text-center _line-2r _smaller-1'>-s6 -m4 -l6</div>
                            <div class='mui-column -s6 -m8 -l6 -green-1-bg -grey-5 _text-center _line-2r _smaller-1'>-s6 -m8 -l6</div>
                        </div>
                        <div class='mui-row'>
                            <div class='mui-column -m6 -l2 -green-5-bg -grey-1 _text-center _line-2r _smaller-1'>-m6 -l2</div>
                            <div class='mui-column -m6 -l10 -green-1-bg -grey-5 _text-center _line-2r _smaller-1'>-m6 -l10</div>
                        </div>
                    </div>

                    <div class='code-example _separate-2 _smaller-1'>
                        <div class='mui-custom-scroll-wrapper'>
                        <div class='mui-custom-scroll -max-height-20 -source'>
                            <pre class='source-code'>
&lt;div class='mui-row'&gt;
    &lt;div class='mui-column -s1'&gt;-s1&lt;/div&gt;
    &lt;div class='mui-column -s11'&gt;-s11&lt;/div&gt;
&lt;/div&gt;
&lt;div class='mui-row'&gt;
    &lt;div class='mui-column -s3'&gt;-s3&lt;/div&gt;
    &lt;div class='mui-column -s9'&gt;-s9&lt;/div&gt;
&lt;/div&gt;
&lt;div class='mui-row'&gt;
    &lt;div class='mui-column -s6 -m4 -l6'&gt;-s6 -m4 -l6&lt;/div&gt;
    &lt;div class='mui-column -s6 -m8 -l6'&gt;-s6 -m8 -l6&lt;/div&gt;
&lt;/div&gt;
&lt;div class='mui-row'&gt;
    &lt;div class='mui-column -m6 -l2'&gt;-m6 -l2&lt;/div&gt;
    &lt;div class='mui-column -m6 -l10'&gt;-m6 -l10&lt;/div&gt;
&lt;/div&gt;</pre>
                        </div>
                        </div>
                    </div>
                </div>
                <!-- /grid overview -->


                <!-- page layout overview -->
                <div class='_separate-5'>
                    <div class='mui-row'>
                        <a class='anchor-link -blue-3 -f-blue-1' href='#page-layout-overview'>
                            <h3 class='_bigger-2' id='page-layout-overview'>Page layout</h3>
                        </a>
                    </div>

                    <p class='mui-p _separate-1'>The basic page layout includes header, page container and footer. This layout is the simplest way to create a sticky footer, but IE11 does not support flexbox greatly, so we have some incorrect behavior in it.</p>

                    <div class='code-example _separate-2 _smaller-1'>
                        <div class='mui-custom-scroll-wrapper'>
                        <div class='mui-custom-scroll -max-height-20 -source'>
                            <pre class='source-code'>
&lt;div class='mui-scroll-fix'&gt;&lt;/div&gt;
&lt;header class='mui-page-header'&gt;header&lt;/header&gt;
&lt;div class='mui-page-container'&gt;...&lt;/div&gt;
&lt;footer class='mui-page-footer'&gt;footer&lt;/footer&gt;</pre>
                        </div>
                        </div>
                    </div>
                </div>
                <!-- /page layout overview -->



                <!-- viewport overview -->
                <div class='_separate-5'>
                    <div class='mui-row'>
                        <a class='anchor-link -blue-3 -f-blue-1' href='#meta-viewport-overview'>
                            <h3 class='_bigger-2' id='meta-viewport'>Recommended viewport</h3>
                        </a>
                    </div>

                    <p class='mui-p _separate-1'>Muilessium uses Hammer.js (exported in Muilessium.DEPENDENCIES), and it is recommended to use the following meta in your pages:</p>

                    <div class='code-example _separate-2 _smaller-1'>
                        <div class='mui-custom-scroll-wrapper'>
                        <div class='mui-custom-scroll -max-height-20 -source'>
                            <pre class='source-code'>
&lt;meta name='viewport' content='user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1'&gt;</pre>
                        </div>
                        </div>
                    </div>
                </div>
                <!-- /viewport overview -->


                <!-- typography overview -->
                <div class='_separate-5'>
                    <div class='mui-row'>
                        <a class='anchor-link -blue-3 -f-blue-1' href='#typography-overview'>
                            <h3 class='_bigger-2' id='typography-overview'>Typography</h3>
                        </a>
                    </div>
                    <div class='_smaller-2 -grey-4'>/src/less/fonts.less</div>

                    <p class='mui-p _separate-1'>Typography in Muilessium is fluid, and it is based on default system fonts and _bigger-* and _smaller-* <a href='#helpers-overview' class='-blue-3 -f-blue-1'>helpers</a>. Note: these helpers have different behavior on different screen sizes to avoid troubles with readability.</p>
                    <div class='h1'>H1 Heading (+5)</div>
                    <div class='h2'>H2 Heading (+4)</div>
                    <div class='h3'>H3 Heading (+3)</div>
                    <div class='h4'>H4 Heading (+2)</div>
                    <div class='h5'>H5 Heading (+1)</div>
                    <div class='h6'>H6 Heading (+0)</div>
                    <div class='_smaller-0'>Normal text</div>
                    <div class='_smaller-1'>Text (-1)</div>
                    <div class='_smaller-2'>Text (-2)</div>
