    <style>
        .mui-page-header {
            padding-left: 1rem;
            /*position: absolute;
            top: 0;
            left: 0;*/
            width: 100%;
            z-index: 1;
        }

        .mui-image.-logo {
            float: left;
            display: inline-block;
            height: 3rem;
            width: 3rem;
            margin: .5rem 1rem .5rem 0;
        }

        .mui-header-logo h1 {
            line-height: 3.7rem;
        }

        .mui-hero-header > .effects {
            background: linear-gradient(to right, #7b4397, #dc2430);
            opacity: .3;
        }

        .mui-header-navigation.-mobile-version .links-list {
            background: #000;
            color: #fff;
        }

        .example .mui-header-navigation.-mobile-version .links-list {
            background: #fff;
            color: #000;
        }

        @media screen and (max-width: 600px) {
            aside {
                display: none !important;
            }
        }

        .aside-navigation {
            padding: 0 1rem;
        }

        .aside-navigation a {
            text-decoration: none;
        }

        .aside-navigation .sub-list {
            list-style: none;
            padding-left: 1em;
        }

        .aside-navigation .sub-list a:before {
            content: '- ';
        }

        .template-preview {
            height: 20rem;
            overflow: hidden;
            padding: 1rem;
        }

        .template-preview a:hover img,
        .template-preview a:focus img {
            opacity: .5;
        }

        .grid-preview .mui-column {
            margin-top: .5rem;
        }

        .grid-preview .mui-column:first-of-type {
            border-right: 2px solid #fff;
        }

        .source-code {
            background: #f1f1f1;
            font-family: Menlo, "Bitstream Vera Sans Mono", "DejaVu Sans Mono", Monaco, Consolas, monospace;
            font-size: .7rem;
            padding: 1rem;
            overflow-x: scroll;
            border-left: 3px solid #777;
        }

        .color-preview {
            display: inline-block !important;
            float: left;
            height: 2rem;
            width: 2rem;
            margin: .3rem;
            border: 1px solid #000;
        }

        .color-name {
            display: inline-block;
            height: 2rem;
            line-height: 2rem;
            padding: 0 .5rem;
            margin: .3rem;
        }

        .example:after {
            content: '';
            display: table;
            clear: both;
        }

        .example > .mui-button {
            margin: 0 1rem 1rem 0;
        }

        .example .mui-header-navigation {
            float: left;
        }

        .questions-list .question {
            font-weight: bold;
        }

        .questions-list .answer {
            font-style: italic;
        }

        .questions-list .question:before {
            content: '- ';
        }

        .questions-list .answer:before {
            content: '- ';
        }

        .mui-carousel,
        .mui-hero-header {
            background: #EDEDED;
        }

        .mui-ribbon-wrapper {
            height: 10rem;
            background: #EDEDED;
        }

        .mui-ribbon-wrapper + .mui-ribbon-wrapper {
            margin-top: 1rem;
        }

        hr {
            border: 0;
            height: 0;
            border-top: 1px solid #c6c6c6;
        }

        .anchor-link {
            text-decoration: none;
        }

        .example > .mui-custom-scroll {
            max-height: 10rem;
            border-top: 1px solid #c6c6c6;
            border-bottom: 1px solid #c6c6c6;
        }

        .page-preview {
            display: block;
            position: relative;
            margin-bottom: 1rem;
            transition: opacity 600ms cubic-bezier(0.53, 0.05, 0.13, 0.94);
        }

        .page-preview > .version {
            text-decoration: none;
            position: absolute;
            top: .5rem;
            right: .5rem;
            padding: 0 .5rem;
        }

        .page-preview:hover,
        .page-preview:focus {
            opacity: .5;
        }

        li > h4 {
            display: inline-block;
        }
    </style>

    <script>
        document.addEventListener('DOMContentLoaded', function() {
            var _ = Muilessium.UTILS;

            function initExamples() {
                _.forEach(document.querySelectorAll('.state .example'), function(example) {
                    var stateClass = _.getAttribute(example, 'data-class'),
                        targetChild = 0;

                    if (example.childNodes[1]) {
                        targetChild = 1;
                    }

                    _.addClass(example.childNodes[targetChild], stateClass);
                });
            }

            initExamples();
        });
    </script>
