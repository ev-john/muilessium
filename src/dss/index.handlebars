    <script>
        document.addEventListener('DOMContentLoaded', function() {
            var _ = Muilessium.Utils;

            function animateTitles() {
                var title1 = document.getElementById('animated-title-1'),
                    title2 = document.getElementById('animated-title-2');

                title1.innerHTML = '';
                title2.innerHTML = '';

                _.typeText(title1, { text: 'MUILESSIUM' }, function() {
                    _.typeText(title2, { text: 'simple should be simple' });
                });
            }


            function initExamples() {
                _.forEach(document.querySelectorAll('.state .example'), function(example) {
                    var stateClass = _.getAttribute(example, 'data-class'),
                        targetChild = 0;

                    if (example.childNodes[1]) {
                        targetChild = 1;
                    }

                    _.addClass(example.childNodes[targetChild], stateClass);
                });
            }

            function hidePreloader() {
                var preloader = document.getElementById('preloader');

                setTimeout(function() {
                    _.addClass(preloader, '-hidden');
                }, 100);
            }

            animateTitles();
            initExamples();
            hidePreloader();
        });
    </script>
